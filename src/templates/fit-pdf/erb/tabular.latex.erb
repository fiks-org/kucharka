<%# tabular environment generated by pandoc %>
<% builder.register_job(
  output_file: meta['filename'] ? meta['filename'] + '.tex' : sha1_hash + '.tex',
  source_file: meta['filename'] ? meta['filename'] : sha1_hash + '.table',
  deploy_dir: meta['deploy_dir'] || builder.template_build_dir,
  command: meta['command'] || "pandoc --from=markdown+simple_tables --to=latex --output=#{meta['filename'] ? meta['filename'] + '.tex' : sha1_hash + '.tex'} #{meta['filename'] ? meta['filename'] : sha1_hash + '.table'}",
  source: strip_indent(dump).strip
) %>
\begingroup
<% if meta['arraystretch'] %>\renewcommand*{\arraystretch}{<%= meta['arraystretch'] %>}<% end %>
\input{<%= meta['filename'] ? meta['filename'] + '.tex' : sha1_hash + '.tex' %>}
\addtocounter{table}{-1} % longtable and table both increases the counter
\endgroup